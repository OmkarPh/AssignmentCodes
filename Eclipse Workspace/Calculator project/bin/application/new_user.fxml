<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="524.0" prefWidth="569.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.CreateUser">
   <children>
      <TextField layoutX="21.0" layoutY="97.0" onInputMethodTextChanged="#checkuser" prefHeight="56.0" prefWidth="264.0" promptText="Enter Username">
         <font>
            <Font size="20.0" />
         </font>
      </TextField>
      <Button fx:id="loginst" layoutX="21.0" layoutY="14.0" mnemonicParsing="false" onMouseClicked="#logininstead" prefHeight="44.0" prefWidth="117.0" text="&lt;-- Login Instead" />
      <PasswordField layoutX="21.0" layoutY="184.0" prefHeight="56.0" prefWidth="278.0" promptText="Create new Password">
         <font>
            <Font size="17.0" />
         </font>
      </PasswordField>
      <PasswordField layoutX="21.0" layoutY="262.0" onInputMethodTextChanged="#checkpass" prefHeight="56.0" prefWidth="278.0" promptText="Re-enter thePassword">
         <font>
            <Font size="20.0" />
         </font>
      </PasswordField>
      <Label fx:id="awb" layoutX="299.0" layoutY="112.0" prefHeight="27.0" prefWidth="128.0" text="Availability check" />
      <Button layoutX="201.0" layoutY="339.0" mnemonicParsing="false" onMouseClicked="#create" prefHeight="86.0" prefWidth="168.0" text="Create User">
         <font>
            <Font size="22.0" />
         </font>
      </Button>
      <Label layoutX="393.0" layoutY="354.0" prefHeight="56.0" prefWidth="168.0" text="You'll have to Login using the &#10;username and password &#10;created just now">
         <font>
            <Font name="System Italic" size="12.0" />
         </font>
      </Label>
      <Label layoutX="313.0" layoutY="277.0" prefHeight="27.0" prefWidth="128.0" text="Similarity check" />
   </children>
</AnchorPane>
